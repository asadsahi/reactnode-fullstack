{
  "name": "reactnode-fullstack",
  "version": "0.0.1",
  "private": true,
  "description": "A Fullstack web application built using React and NodeJs with relational database backend.",
  "repository": {
    "type": "git",
    "url": "git://github.com/asadsahi/reactnode-fullstack.git"
  },
  "engines": {
    "npm": ">=5",
    "node": ">=8"
  },
  "author": "Asad Sahi",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon",
    "start": "cross-env NODE_ENV=production node --inspect scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js",
    "e2e": "testcafe chrome e2e/**/*.e2e-spec.js",
    "e2e:watch": "testcafe-live chrome e2e/**/*.e2e-spec.js",
    "e2e:ci": "testcafe all e2e/**/*.e2e-spec.js --app \"yarn dev\" --app-init-delay 4000",
    "db": "sequelize db:migrate && sequelize db:seed:all",
    "heroku-postbuild": "npm run build && npm run db"
  },
  "dependencies": {
    "@babel/core": "7.3.4",
    "@babel/polyfill": "7.2.5",
    "@babel/preset-env": "7.3.4",
    "@babel/preset-react": "7.0.0",
    "@babel/register": "7.0.0",
    "@svgr/webpack": "4.1.0",
    "acl": "0.4.11",
    "axios": "0.18.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "24.3.1",
    "babel-loader": "8.0.5",
    "babel-plugin-named-asset-import": "0.3.1",
    "babel-preset-react-app": "7.0.2",
    "bfj": "6.1.1",
    "body-parser": "1.18.3",
    "bootstrap": "4.3.1",
    "case-sensitive-paths-webpack-plugin": "2.2.0",
    "chalk": "2.4.2",
    "classnames": "2.2.6",
    "compression": "1.7.3",
    "cookie-parser": "1.4.4",
    "cross-env": "5.2.0",
    "css-loader": "2.1.1",
    "dotenv": "6.2.0",
    "dotenv-expand": "4.2.0",
    "eslint": "5.15.1",
    "eslint-config-prettier": "4.1.0",
    "eslint-config-react-app": "3.0.8",
    "eslint-loader": "2.1.2",
    "eslint-plugin-flowtype": "3.4.2",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-react": "7.12.4",
    "eslint-plugin-testcafe": "0.2.1",
    "eventsource-polyfill": "0.9.6",
    "express": "4.16.4",
    "express-jwt": "5.3.1",
    "file-loader": "3.0.1",
    "final-form": "4.12.0",
    "font-awesome": "4.7.0",
    "fork-ts-checker-webpack-plugin-alt": "0.4.14",
    "fs-extra": "7.0.1",
    "globby": "9.1.0",
    "helmet": "3.15.1",
    "history": "4.7.2",
    "html-webpack-plugin": "4.0.0-alpha.2",
    "identity-obj-proxy": "3.0.0",
    "jest": "24.3.1",
    "jest-pnp-resolver": "1.2.0",
    "jest-resolve": "24.3.1",
    "jwks-rsa": "1.4.0",
    "lodash": "4.17.11",
    "mini-css-extract-plugin": "0.5.0",
    "moment": "2.24.0",
    "morgan": "1.9.1",
    "multer": "1.4.1",
    "node-sass": "4.11.0",
    "nodemailer": "5.1.1",
    "oidc-client": "1.6.1",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "pnp-webpack-plugin": "1.4.1",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "postcss-preset-env": "6.6.0",
    "postcss-safe-parser": "4.0.1",
    "prettier": "1.16.4",
    "react": "16.8.4",
    "react-app-polyfill": "0.2.2",
    "react-big-calendar": "0.20.3",
    "react-dates": "20.0.0",
    "react-datetime": "2.16.3",
    "react-day-picker": "7.3.0",
    "react-dev-utils": "8.0.0",
    "react-dom": "16.8.4",
    "react-final-form": "4.1.0",
    "react-helmet": "5.2.0",
    "react-router-dom": "4.3.1",
    "react-table": "6.9.2",
    "reactstrap": "7.1.0",
    "resolve": "1.10.0",
    "sass-loader": "7.1.0",
    "sequelize": "4.43.0",
    "sequelize-cli": "5.4.0",
    "socket.io": "2.2.0",
    "socket.io-client": "2.2.0",
    "sqlite3": "4.0.6",
    "style-loader": "0.23.1",
    "terser-webpack-plugin": "1.2.3",
    "testcafe": "1.1.0",
    "testcafe-live": "0.1.4",
    "url-loader": "1.1.2",
    "webpack": "4.29.6",
    "webpack-bundle-analyzer": "3.1.0",
    "webpack-dev-middleware": "3.6.1",
    "webpack-hot-middleware": "2.24.3",
    "webpack-manifest-plugin": "2.0.4",
    "workbox-webpack-plugin": "4.1.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "resolver": "jest-pnp-resolver",
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "testMatch": [
      "<rootDir>/src/**/*.(spec|test).{js,jsx,ts,tsx}",
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  }
}
