{
  "name": "reactnode-fullstack",
  "version": "0.0.1",
  "private": true,
  "description": "A Fullstack web application built using React and NodeJs with relational database backend.",
  "repository": {
    "type": "git",
    "url": "git://github.com/asadsahi/reactnode-fullstack.git"
  },
  "engines": {
    "npm": ">=5",
    "node": ">=8"
  },
  "author": "Asad Sahi",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon",
    "start": "cross-env NODE_ENV=production node --inspect scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js",
    "e2e": "testcafe chrome e2e/**/*.e2e-spec.js",
    "e2e:watch": "testcafe-live chrome e2e/**/*.e2e-spec.js",
    "e2e:ci": "testcafe all e2e/**/*.e2e-spec.js --app \"yarn dev\" --app-init-delay 4000",
    "db": "sequelize db:migrate && sequelize db:seed:all",
    "heroku-postbuild": "npm run build && npm run db"
  },
  "dependencies": {
    "@babel/core": "7.7.5",
    "@babel/polyfill": "7.7.0",
    "@babel/preset-env": "7.7.6",
    "@babel/preset-react": "7.7.4",
    "@babel/register": "7.7.4",
    "@svgr/webpack": "4.3.3",
    "acl": "0.4.11",
    "axios": "0.19.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.3",
    "babel-jest": "24.9.0",
    "babel-loader": "8.0.6",
    "babel-plugin-named-asset-import": "0.3.5",
    "babel-preset-react-app": "9.1.0",
    "bfj": "7.0.1",
    "body-parser": "1.19.0",
    "bootstrap": "4.4.1",
    "case-sensitive-paths-webpack-plugin": "2.2.0",
    "chalk": "3.0.0",
    "classnames": "2.2.6",
    "compression": "1.7.4",
    "cookie-parser": "1.4.4",
    "cross-env": "6.0.3",
    "css-loader": "3.4.0",
    "dotenv": "8.2.0",
    "dotenv-expand": "5.1.0",
    "eslint": "6.7.2",
    "eslint-config-prettier": "6.7.0",
    "eslint-config-react-app": "5.1.0",
    "eslint-loader": "3.0.3",
    "eslint-plugin-flowtype": "4.5.2",
    "eslint-plugin-import": "2.19.1",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-react": "7.17.0",
    "eslint-plugin-react-hooks": "2.3.0",
    "eslint-plugin-testcafe": "0.2.1",
    "eventsource-polyfill": "0.9.6",
    "express": "4.17.1",
    "express-jwt": "5.3.1",
    "file-loader": "5.0.2",
    "final-form": "4.18.6",
    "font-awesome": "4.7.0",
    "fork-ts-checker-webpack-plugin-alt": "0.4.14",
    "fs-extra": "8.1.0",
    "globby": "10.0.1",
    "helmet": "3.21.2",
    "history": "4.10.1",
    "html-webpack-plugin": "4.0.0-alpha.2",
    "identity-obj-proxy": "3.0.0",
    "jest": "24.9.0",
    "jest-pnp-resolver": "1.2.1",
    "jest-resolve": "24.9.0",
    "jwks-rsa": "1.6.0",
    "lodash": "4.17.15",
    "mini-css-extract-plugin": "0.8.2",
    "moment": "2.24.0",
    "morgan": "1.9.1",
    "multer": "1.4.2",
    "node-sass": "4.13.0",
    "nodemailer": "6.4.2",
    "oidc-client": "1.10.0",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "pnp-webpack-plugin": "1.5.0",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "postcss-preset-env": "6.7.0",
    "postcss-safe-parser": "4.0.1",
    "prettier": "1.19.1",
    "react": "16.12.0",
    "react-app-polyfill": "1.0.5",
    "react-big-calendar": "0.23.0",
    "react-dates": "21.5.1",
    "react-datetime": "2.16.3",
    "react-day-picker": "7.4.0",
    "react-dev-utils": "10.0.0",
    "react-dom": "16.12.0",
    "react-final-form": "6.3.3",
    "react-helmet": "5.2.1",
    "react-router-dom": "5.1.2",
    "react-table": "6.11.5",
    "reactstrap": "8.2.0",
    "resolve": "1.14.0",
    "sass-loader": "8.0.0",
    "sequelize": "5.21.3",
    "sequelize-cli": "5.5.1",
    "socket.io": "2.3.0",
    "socket.io-client": "2.3.0",
    "sqlite3": "4.1.1",
    "style-loader": "1.0.2",
    "terser-webpack-plugin": "2.3.1",
    "testcafe": "1.7.0",
    "testcafe-live": "0.1.4",
    "url-loader": "3.0.0",
    "webpack": "4.41.3",
    "webpack-bundle-analyzer": "3.6.0",
    "webpack-dev-middleware": "3.7.2",
    "webpack-hot-middleware": "2.25.0",
    "webpack-manifest-plugin": "2.2.0",
    "workbox-webpack-plugin": "4.3.1"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "resolver": "jest-pnp-resolver",
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "testMatch": [
      "<rootDir>/src/**/*.(spec|test).{js,jsx,ts,tsx}",
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  }
}
