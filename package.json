{
  "name": "reactnode-fullstack",
  "version": "0.0.1",
  "description": "A Fullstack web application built using React and NodeJs with relational database backend.",
  "repository": {
    "type": "git",
    "url": "git://github.com/asadsahi/reactnode-fullstack.git"
  },
  "engines": {
    "npm": ">=3",
    "node": ">=6 <9"
  },
  "author": "Asad Sahi",
  "license": "MIT",
  "scripts": {
    "precommit": "lint-staged",
    "analyze": "cross-env BUNDLE_ANALYZE=true npm run build",
    "npmcheckversion": "node ./config/scripts/npmcheckversion.js",
    "preinstall": "npm run npmcheckversion",
    "prebuild": "npm run build:clean",
    "build": "cross-env NODE_ENV=production webpack --config config/webpack/webpack.prod.babel.js --color --progress --hide-modules --display-optimization-bailout",
    "build:clean": "rimraf ./build",
    "dev": "npm run start",
    "start": "cross-env NODE_ENV=development nodemon",
    "clean:start": "npm run clean && npm run db && cross-env NODE_ENV=development nodemon",
    "start:tunnel": "cross-env NODE_ENV=development ENABLE_TUNNEL=true nodemon",
    "start:production": "npm run test && npm run build && npm run start:prod",
    "start:prod": "cross-env NODE_ENV=production node server/server",
    "clean": "npm run analyze:clean && npm run test:clean && npm run build:clean && rimraf db.sqlite",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "eslint . --fix",
    "lint:css": "stylelint \"./app/**/*.css\"",
    "pretest": "npm run test:clean && npm run lint",
    "test:clean": "rimraf ./coverage",
    "test": "cross-env NODE_ENV=test jest --coverage",
    "test:dev": "cross-env NODE_ENV=test jest --watch",
    "test:watch": "cross-env NODE_ENV=test jest --watchAll",
    "e2e": "testcafe chrome e2e/**/*.e2e-spec.js",
    "e2e:watch": "testcafe-live chrome e2e/**/*.e2e-spec.js",
    "e2e:ci": "testcafe all e2e/**/*.e2e-spec.js --app \"yarn dev\" --app-init-delay 4000",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "db": "sequelize db:migrate && sequelize db:seed:all",
    "heroku-postbuild": "npm run build && npm run db"
  },
  "lint-staged": {
    "*.js": [
      "npm run lint:js"
    ]
  },
  "babel": {
    "plugins": [],
    "presets": [
      [
        "env",
        {
          "modules": false
        }
      ],
      "react",
      "stage-0"
    ],
    "env": {
      "development": {
        "only": [
          "app",
          "config/scripts"
        ],
        "plugins": [
          "transform-react-jsx-source"
        ]
      },
      "production": {
        "only": [
          "app"
        ],
        "plugins": [
          "transform-react-remove-prop-types",
          "transform-react-inline-elements",
          "transform-react-constant-elements"
        ]
      },
      "test": {
        "plugins": [
          "transform-es2015-modules-commonjs"
        ]
      }
    }
  },
  "stylelint": {
    "rules": {
      "comment-empty-line-before": "never"
    },
    "processors": [],
    "extends": [
      "stylelint-config-standard"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "app/**/*.{js,jsx}",
      "!app/**/*.test.{js,jsx}",
      "!app/*/RbGenerated*/*.{js,jsx}",
      "!app/app.js",
      "!app/global-styles.js",
      "!app/*/*/Loadable.{js,jsx}"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 0,
        "branches": 0,
        "functions": 0,
        "lines": 0
      }
    },
    "moduleDirectories": [
      "node_modules",
      "app"
    ],
    "moduleNameMapper": {
      ".*\\.(css|less|styl|scss|sass)$": "<rootDir>/config/testing/mocks/cssModule.js",
      ".*\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/config/testing/mocks/image.js"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/config/testing/test-bundler.js",
    "setupFiles": [
      "raf/polyfill",
      "<rootDir>/config/testing/enzyme-setup.js"
    ]
  },
  "dependencies": {
    "acl": "0.4.11",
    "axios": "0.18.0",
    "babel-polyfill": "6.26.0",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "1.18.3",
    "chalk": "2.4.1",
    "classnames": "2.2.6",
    "compression": "1.7.2",
    "cookie-parser": "1.4.3",
    "cors": "2.8.4",
    "cross-env": "5.2.0",
    "eslint-config-prettier": "2.9.0",
    "express": "4.16.3",
    "formik": "0.11.11",
    "globby": "8.0.1",
    "helmet": "3.12.1",
    "history": "4.7.2",
    "ignore-styles": "5.0.1",
    "immutable": "3.8.2",
    "ip": "1.1.5",
    "jsonwebtoken": "8.3.0",
    "lodash": "4.17.10",
    "minimist": "1.2.0",
    "morgan": "1.9.0",
    "multer": "1.3.1",
    "nodemailer": "4.6.7",
    "passport": "0.4.0",
    "passport-facebook": "2.1.1",
    "passport-google-oauth": "1.0.0",
    "passport-jwt": "4.0.0",
    "passport-local": "1.0.0",
    "react": "16.4.1",
    "react-dom": "16.4.1",
    "react-helmet": "5.2.0",
    "react-router-dom": "4.3.1",
    "sanitize.css": "6.0.0",
    "sequelize": "4.38.0",
    "sequelize-cli": "4.0.0",
    "socket.io": "2.1.1",
    "socket.io-client": "2.1.1",
    "sqlite3": "4.0.1",
    "yup": "0.25.1"
  },
  "devDependencies": {
    "add-asset-html-webpack-plugin": "2.1.3",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-eslint": "8.2.5",
    "babel-jest": "23.2.0",
    "babel-loader": "7.1.4",
    "babel-plugin-react-transform": "3.0.0",
    "babel-plugin-transform-es2015-modules-commonjs": "6.26.2",
    "babel-plugin-transform-react-constant-elements": "6.23.0",
    "babel-plugin-transform-react-inline-elements": "6.22.0",
    "babel-plugin-transform-react-jsx-source": "6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.13",
    "babel-preset-env": "1.7.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "circular-dependency-plugin": "5.0.2",
    "coveralls": "3.0.1",
    "css-loader": "0.28.11",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "eslint": "5.0.1",
    "eslint-config-airbnb": "17.0.0",
    "eslint-config-airbnb-base": "13.0.0",
    "eslint-import-resolver-webpack": "0.10.1",
    "eslint-plugin-import": "2.13.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-prettier": "2.6.1",
    "eslint-plugin-react": "7.10.0",
    "eslint-plugin-testcafe": "0.2.1",
    "eventsource-polyfill": "0.9.6",
    "exports-loader": "0.7.0",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "file-loader": "1.1.11",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "husky": "0.14.3",
    "image-webpack-loader": "4.3.1",
    "imports-loader": "0.8.0",
    "jest-cli": "23.2.0",
    "lint-staged": "7.2.0",
    "ngrok": "3.0.1",
    "nock": "9.3.3",
    "node-sass": "4.9.0",
    "nodemon": "1.17.5",
    "null-loader": "0.1.1",
    "offline-plugin": "5.0.5",
    "prettier": "1.13.6",
    "raf": "3.4.0",
    "react-test-renderer": "16.4.1",
    "rimraf": "2.6.2",
    "sass-loader": "7.0.3",
    "shelljs": "0.8.2",
    "style-loader": "0.21.0",
    "stylelint": "9.3.0",
    "stylelint-config-standard": "18.2.0",
    "testcafe": "0.20.4",
    "testcafe-live": "0.1.3",
    "url-loader": "1.0.1",
    "webpack": "4.12.2",
    "webpack-bundle-analyzer": "2.13.1",
    "webpack-cli": "3.0.8",
    "webpack-dev-middleware": "3.1.3",
    "webpack-hot-middleware": "2.22.2"
  }
}
