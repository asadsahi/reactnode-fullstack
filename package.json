{
  "name": "reactnode-fullstack",
  "version": "0.0.1",
  "description": "A Fullstack web application built using React and NodeJs with relational database backend.",
  "repository": {
    "type": "git",
    "url": "git://github.com/asadsahi/reactnode-fullstack.git"
  },
  "engines": {
    "npm": ">=5",
    "node": ">=8"
  },
  "author": "Asad Sahi",
  "license": "MIT",
  "scripts": {
    "analyze": "cross-env BUNDLE_ANALYZE=true npm run build",
    "npmcheckversion": "node ./config/scripts/npmcheckversion.js",
    "preinstall": "npm run npmcheckversion",
    "prebuild": "npm run build:clean",
    "build": "cross-env NODE_ENV=production webpack --config config/webpack/webpack.prod.babel.js --color --progress --hide-modules --display-optimization-bailout",
    "build:clean": "rimraf ./build",
    "dev": "npm run start",
    "start": "cross-env NODE_ENV=development nodemon",
    "clean:start": "npm run clean && npm run db && cross-env NODE_ENV=development nodemon",
    "start:tunnel": "cross-env NODE_ENV=development ENABLE_TUNNEL=true nodemon",
    "start:production": "npm run test && npm run build && npm run start:prod",
    "start:prod": "cross-env NODE_ENV=production node server/server",
    "clean": "npm run analyze:clean && npm run test:clean && npm run build:clean && rimraf db.sqlite",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "eslint . --fix",
    "lint:css": "stylelint \"./app/**/*.css\"",
    "pretest": "npm run test:clean && npm run lint",
    "test:clean": "rimraf ./coverage",
    "test": "cross-env NODE_ENV=test jest --coverage",
    "test:dev": "cross-env NODE_ENV=test jest --watch",
    "test:watch": "cross-env NODE_ENV=test jest --watchAll",
    "e2e": "testcafe chrome e2e/**/*.e2e-spec.js",
    "e2e:watch": "testcafe-live chrome e2e/**/*.e2e-spec.js",
    "e2e:ci": "testcafe all e2e/**/*.e2e-spec.js --app \"yarn dev\" --app-init-delay 4000",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "db": "sequelize db:migrate && sequelize db:seed:all",
    "heroku-postbuild": "npm run build && npm run db"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run lint:js"
    ]
  },
  "babel": {
    "plugins": [
      "@babel/plugin-syntax-dynamic-import",
      "@babel/plugin-syntax-import-meta",
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-json-strings",
      [
        "@babel/plugin-proposal-decorators",
        {
          "legacy": true
        }
      ],
      "@babel/plugin-proposal-function-sent",
      "@babel/plugin-proposal-export-namespace-from",
      "@babel/plugin-proposal-numeric-separator",
      "@babel/plugin-proposal-throw-expressions",
      "@babel/plugin-proposal-export-default-from",
      "@babel/plugin-proposal-logical-assignment-operators",
      "@babel/plugin-proposal-optional-chaining",
      [
        "@babel/plugin-proposal-pipeline-operator",
        {
          "proposal": "minimal"
        }
      ],
      "@babel/plugin-proposal-nullish-coalescing-operator",
      "@babel/plugin-proposal-do-expressions",
      "@babel/plugin-proposal-function-bind"
    ],
    "presets": [
      [
        "@babel/preset-env",
        {
          "modules": false
        }
      ],
      "@babel/preset-react"
    ],
    "env": {
      "development": {
        "only": [
          "app",
          "config/scripts"
        ],
        "plugins": [
          "@babel/plugin-transform-react-jsx-source"
        ]
      },
      "production": {
        "only": [
          "app"
        ],
        "plugins": [
          "transform-react-remove-prop-types",
          "@babel/plugin-transform-react-inline-elements",
          "@babel/plugin-transform-react-constant-elements"
        ]
      },
      "test": {
        "plugins": [
          "@babel/plugin-transform-modules-commonjs"
        ]
      }
    }
  },
  "stylelint": {
    "rules": {
      "comment-empty-line-before": "never"
    },
    "processors": [],
    "extends": [
      "stylelint-config-standard"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "app/**/*.{js,jsx}",
      "!app/**/*.test.{js,jsx}",
      "!app/*/RbGenerated*/*.{js,jsx}",
      "!app/app.js",
      "!app/global-styles.js",
      "!app/*/*/Loadable.{js,jsx}"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 0,
        "branches": 0,
        "functions": 0,
        "lines": 0
      }
    },
    "moduleDirectories": [
      "node_modules",
      "app"
    ],
    "moduleNameMapper": {
      ".*\\.(css|less|styl|scss|sass)$": "<rootDir>/config/testing/mocks/cssModule.js",
      ".*\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/config/testing/mocks/image.js"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/config/testing/test-bundler.js",
    "setupFiles": [
      "raf/polyfill",
      "<rootDir>/config/testing/enzyme-setup.js"
    ]
  },
  "dependencies": {
    "@babel/polyfill": "7.0.0",
    "@babel/register": "7.0.0",
    "acl": "0.4.11",
    "axios": "0.18.0",
    "body-parser": "1.18.3",
    "bootstrap": "4.1.3",
    "chalk": "2.4.1",
    "classnames": "2.2.6",
    "compression": "1.7.3",
    "cookie-parser": "1.4.3",
    "cross-env": "5.2.0",
    "date-fns": "1.29.0",
    "eslint-config-prettier": "3.1.0",
    "express": "4.16.4",
    "express-jwt": "5.3.1",
    "final-form": "4.10.0",
    "font-awesome": "4.7.0",
    "globby": "8.0.1",
    "helmet": "3.14.0",
    "history": "4.7.2",
    "ignore-styles": "5.0.1",
    "ip": "1.1.5",
    "jwks-rsa": "1.3.0",
    "lodash": "4.17.11",
    "minimist": "1.2.0",
    "morgan": "1.9.1",
    "multer": "1.4.1",
    "nodemailer": "4.6.8",
    "oidc-client": "1.5.3-beta.1",
    "react": "16.5.2",
    "react-dom": "16.5.2",
    "react-final-form": "3.6.7",
    "react-helmet": "5.2.0",
    "react-router-dom": "4.3.1",
    "sequelize": "4.39.1",
    "sequelize-cli": "5.1.0",
    "socket.io": "2.1.1",
    "socket.io-client": "2.1.1",
    "sqlite3": "4.0.2"
  },
  "devDependencies": {
    "@babel/cli": "7.1.2",
    "@babel/core": "7.1.2",
    "@babel/plugin-proposal-class-properties": "7.1.0",
    "@babel/plugin-proposal-decorators": "7.1.2",
    "@babel/plugin-proposal-do-expressions": "7.0.0",
    "@babel/plugin-proposal-export-default-from": "7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "7.0.0",
    "@babel/plugin-proposal-function-bind": "7.0.0",
    "@babel/plugin-proposal-function-sent": "7.1.0",
    "@babel/plugin-proposal-json-strings": "7.0.0",
    "@babel/plugin-proposal-logical-assignment-operators": "7.0.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.0.0",
    "@babel/plugin-proposal-numeric-separator": "7.0.0",
    "@babel/plugin-proposal-optional-chaining": "7.0.0",
    "@babel/plugin-proposal-pipeline-operator": "7.0.0",
    "@babel/plugin-proposal-throw-expressions": "7.0.0",
    "@babel/plugin-syntax-dynamic-import": "7.0.0",
    "@babel/plugin-syntax-import-meta": "7.0.0",
    "@babel/plugin-transform-modules-commonjs": "7.1.0",
    "@babel/plugin-transform-react-constant-elements": "7.0.0",
    "@babel/plugin-transform-react-inline-elements": "7.0.0",
    "@babel/plugin-transform-react-jsx-source": "7.0.0",
    "@babel/preset-env": "7.1.0",
    "@babel/preset-react": "7.0.0",
    "add-asset-html-webpack-plugin": "3.1.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.4",
    "babel-plugin-react-transform": "3.0.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.19",
    "circular-dependency-plugin": "5.0.2",
    "coveralls": "3.0.2",
    "css-loader": "1.0.0",
    "enzyme": "3.7.0",
    "enzyme-adapter-react-16": "1.6.0",
    "eslint": "5.7.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-import-resolver-webpack": "0.10.1",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-prettier": "3.0.0",
    "eslint-plugin-react": "7.11.1",
    "eslint-plugin-testcafe": "0.2.1",
    "eventsource-polyfill": "0.9.6",
    "exports-loader": "0.7.0",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "file-loader": "2.0.0",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "husky": "1.1.2",
    "image-webpack-loader": "4.4.0",
    "imports-loader": "0.8.0",
    "jest-cli": "23.6.0",
    "lint-staged": "7.3.0",
    "ngrok": "3.1.0",
    "nock": "10.0.1",
    "node-sass": "4.9.4",
    "nodemon": "1.18.4",
    "null-loader": "0.1.1",
    "offline-plugin": "^5.0.5",
    "prettier": "1.14.3",
    "raf": "3.4.0",
    "react-test-renderer": "16.5.2",
    "rimraf": "2.6.2",
    "sass-loader": "7.1.0",
    "shelljs": "0.8.2",
    "style-loader": "0.23.1",
    "stylelint": "9.6.0",
    "stylelint-config-standard": "18.2.0",
    "testcafe": "0.22.0",
    "testcafe-live": "0.1.3",
    "url-loader": "1.1.2",
    "webpack": "4.20.2",
    "webpack-bundle-analyzer": "3.0.3",
    "webpack-cli": "3.1.2",
    "webpack-dev-middleware": "3.4.0",
    "webpack-hot-middleware": "2.24.3"
  }
}
